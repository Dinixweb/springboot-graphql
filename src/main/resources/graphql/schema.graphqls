type  Query{
    students:[Students]
}
type Students{
    id:ID
    firstName:String!
    lastName:String!
    grade:String!
    subjects:[Subjects]
    guardian:[Guardian]
}

type Guardian {
    firstName:String!
    lastname:String!
    email:String!

}

type Subjects {
    subjectId:ID!
    subjectName:String!
    subjectTeacher:String!
}
